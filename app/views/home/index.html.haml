%section#intro
  .gridwrap
    #slideshow.grid-1-1.no-padding
      %img{:src => "http://flockcapital.com/img/xsf-housing.jpg.pagespeed.ic.LgUeSLDJBi.jpg", :style => "margin: 0px;"}
%section.home-listing
  .listing-head
    %h4
      %label Local
      %span= link_to "See all Local", projects_path
  .gridwrap
    = render @projects
    %table.table.table-hover
      %thead
        %tr
          %th Name
          %th Email

      %tbody
        = render partial: 'user', collection: @users
